import difflib

# List of values
values = ['aku', 'nomor rekening', '2000222', 'account number', '20009']

# Keywords to match
keywords = ['no rekening', 'account no']

def find_closest_matches(values, keywords):
    # Normalize keywords by removing spaces and converting to lower case
    normalized_keywords = [keyword.replace(' ', '').lower() for keyword in keywords]

    # List to store matched values
    matched_values = []

    for value in values:
        # Normalize value
        normalized_value = value.replace(' ', '').lower()
        
        # Calculate closest match using difflib
        closest_match = difflib.get_close_matches(normalized_value, normalized_keywords, n=1, cutoff=0.6)
        
        if closest_match:
            # If a match is found, check if the value is numeric and add to matched values
            if value.isnumeric():
                matched_values.append(value)
    
    return matched_values

# Find and print the matched values
matched_values = find_closest_matches(values, keywords)
print(matched_values)
